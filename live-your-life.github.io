    <img src = "https://upload.wikimedia.org/wikipedia/commons/d/db/Ac.buddhists.jpg" alt="picture of a golden statue" width ="180">
    <img src = "https://upload.wikimedia.org/wikipedia/commons/e/e9/Isla_Phi_Phi_Lay%2C_Tailandia%2C_2013-08-19%2C_DD_07.JPG" alt ="picture of a beach in thailand " width ="196">
    <img src = "https://upload.wikimedia.org/wikipedia/commons/e/e7/KohPhiPhi.JPG" alt="picture of a beach in thailand " width ="180">
    <img src = "https://upload.wikimedia.org/wikipedia/commons/thumb/d/db/2005-9usi-thai.jpg/1024px-2005-9usi-thai.jpg" alt="picture of a woman herding her cow" width ="198">
